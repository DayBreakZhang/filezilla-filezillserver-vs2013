VS_VERSION_INFO VERSIONINFO
 FILEVERSION @PACKAGE_VERSION_MAJOR@,@PACKAGE_VERSION_MINOR@,@PACKAGE_VERSION_MICRO@,@PACKAGE_VERSION_NANO@
 PRODUCTVERSION @PACKAGE_VERSION_MAJOR@,@PACKAGE_VERSION_MINOR@,@PACKAGE_VERSION_MICRO@,@PACKAGE_VERSION_NANO@
 FILEFLAGSMASK 0x17L
 FILEFLAGS 0x0L
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "000004b0"
        BEGIN
            VALUE "Comments", "Version @PACKAGE_VERSION@"
            VALUE "CompanyName", "FileZilla Project"
            VALUE "FileDescription", "FileZilla FTP Client"
            VALUE "FileVersion", "@PACKAGE_VERSION_MAJOR@, @PACKAGE_VERSION_MINOR@, @PACKAGE_VERSION_MICRO@, @PACKAGE_VERSION_NANO@"
            VALUE "InternalName", "FileZilla 3"
            VALUE "LegalCopyright", "Copyright (C) 2006-2015"
            VALUE "OriginalFilename", "filezilla.exe"
            VALUE "ProductName", "FileZilla"
            VALUE "ProductVersion", "@PACKAGE_VERSION_MAJOR@, @PACKAGE_VERSION_MINOR@, @PACKAGE_VERSION_MICRO@, @PACKAGE_VERSION_NANO@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0, 1200
    END
END

